<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lacrastemalhariafx.OpTecelagemController">
   <children>
      <Pane prefHeight="550.0" prefWidth="750.0">
         <children>
            <ImageView fitHeight="550.0" fitWidth="750.0">
               <image>
                  <Image url="@fundo.jpg" />
               </image>
            </ImageView>
            <Button fx:id="cadasTece" layoutX="250.0" layoutY="198.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="250.0" style="-fx-background-radius: 50; -fx-background-color: #b86786;" text="Checar dados de cadastro" textFill="WHITE">
               
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="fiosTece" layoutX="253.0" layoutY="275.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="250.0" style="-fx-background-radius: 50; -fx-background-color: #b86786;" text="Checar tipos de fios" textFill="WHITE">
               
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="maqTece" layoutX="253.0" layoutY="355.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="250.0" style="-fx-background-radius: 50; -fx-background-color: #b86786;" text="Checar maquinário" textFill="WHITE">
              
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="inicTece" graphicTextGap="5.0" layoutX="31.0" layoutY="465.0" onAction="#botaoInicial" prefHeight="61.0" prefWidth="69.0" style="-fx-background-color: #cf8fa7; -fx-background-radius: 50;" text="Início" textAlignment="CENTER" textFill="#ffffff">
              
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <ImageView fitHeight="158.0" fitWidth="353.0" layoutX="-29.0">
               <image>
                  <Image url="@note.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Button fx:id="voltTece" graphicTextGap="5.0" layoutX="666.0" layoutY="465.0" onAction="#OpcoesFuncio" prefHeight="70.0" prefWidth="70.0" style="-fx-background-color: #cf8fa7; -fx-background-radius: 50;" text="Voltar" textAlignment="CENTER" textFill="#f6efef">
        
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
